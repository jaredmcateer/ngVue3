    "@types/angular": "^1.8.4",
    "@types/angular-mocks": "^1.7.1",
    "@types/jest": "^27.4.1",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/test-utils": "^2.0.0-rc.21",
    "@vue/vue3-jest": "^27.0.0",
    "angular": "^1.8.3",
    "angular-mocks": "^1.8.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "27.5.1",
    "config": "workspace:*",
    "eslint": "^8.14.0",
    "global-jsdom": "^8.4.0",
    "jest": "27.5.1",
    "jest-jasmine2": "^27.1.4",
    "jsdom": "^19.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "tsconfig": "workspace:*",
    "typescript": "^4.5.4",
    "vite": "^2.9.5",
    "vue": "^3.2.25",
    "vue-tsc": "^0.34.7"
