    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "javascript",
    "typescript",
    "migration",
    "vue",
    "vue3",
    "angular",
    "ng",
    "components",
    "ngvue",
    "ngvue3"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaredmcateer/ngVue3.git"
  },
  "main": "./dist/main.mjs",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.cjs"
